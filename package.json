{
  "name": "template-nodejs-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/",
    "dev": "nodemon",
    "start:dev": "tsc --watch",
    "start": "node ./dist/index.js",
    "compile": "npm run clean && tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json"
  },
  "author": "npv2k1",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.19",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.2.1"
  }
}